datasource db {
    provider = "postgresql"
    url = "***"
}

model Ong {
    id  Int @id @default(autoincrement())
    name    String
    email   String @unique
    password    String
    uf  String
    city String
    whatsapp    String
}

model Incident {
    id  Int @id @default(autoincrement())
    title String
    description String
    value   Float
    ong     Ong 
}

generator client {
  provider = "prisma-client-js"
}